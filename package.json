{
  "name": "form-builder",
  "version": "1.0.0",
  "homepage": "https://designerofcontent.github.io/form-builder",
  "description": "A modern form builder application similar to Paperform",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
